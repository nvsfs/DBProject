  <section class="main clearfix">

<%- model_class = Search -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:nome_equip_urbano) %></th>
      <th><%= model_class.human_attribute_name(:tipo_equip_urbano) %></th>
      <th><%= model_class.human_attribute_name(:endereco_equip_urbano) %></th>
      <th><%= model_class.human_attribute_name(:codigo_logradouro) %></th>
      <th><%= model_class.human_attribute_name(:lei_equip_urbano) %></th>
      <th><%= model_class.human_attribute_name(:nome_bairro) %></th>
      <th><%= model_class.human_attribute_name(:codigo_bairro) %></th>
      <th><%= model_class.human_attribute_name(:nome_oficial_equip_urbano) %></th>
      <th><%= model_class.human_attribute_name(:area) %></th>
      <th><%= model_class.human_attribute_name(:perimetro) %></th>
      <th><%= model_class.human_attribute_name(:latitude) %></th>
      <th><%= model_class.human_attribute_name(:longitude) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @pracas.where("tipo_equip_urbano = 'area verde'") %>
      <tr>
        <td><%= link_to praca.id, praca_path(praca) %></td>
        <td><%= praca.nome_equip_urbano %></td>
        <td><%= praca.tipo_equip_urbano %></td>
        <td><%= praca.endereco_equip_urbano %></td>
        <td><%= praca.codigo_logradouro %></td>
        <td><%= praca.lei_equip_urbano %></td>
        <td><%= praca.nome_bairro %></td>
        <td><%= praca.codigo_bairro %></td>
        <td><%= praca.nome_oficial_equip_urbano %></td>
        <td><%= praca.area %></td>
        <td><%= praca.perimetro %></td>
        <td><%= praca.latitude %></td>
        <td><%= praca.longitude %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_praca_path(praca), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      praca_path(praca),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_praca_path,
            :class => 'btn btn-primary' %>
</section>