  <section class="main clearfix">
<%- model_class = Praca -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome_equip_urbano) %>:</strong></dt>
  <dd><%= @praca.nome_equip_urbano %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_equip_urbano) %>:</strong></dt>
  <dd><%= @praca.tipo_equip_urbano %></dd>
  <dt><strong><%= model_class.human_attribute_name(:endereco_equip_urbano) %>:</strong></dt>
  <dd><%= @praca.endereco_equip_urbano %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codigo_logradouro) %>:</strong></dt>
  <dd><%= @praca.codigo_logradouro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lei_equip_urbano) %>:</strong></dt>
  <dd><%= @praca.lei_equip_urbano %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome_bairro) %>:</strong></dt>
  <dd><%= @praca.nome_bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codigo_bairro) %>:</strong></dt>
  <dd><%= @praca.codigo_bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome_oficial_equip_urbano) %>:</strong></dt>
  <dd><%= @praca.nome_oficial_equip_urbano %></dd>
  <dt><strong><%= model_class.human_attribute_name(:area) %>:</strong></dt>
  <dd><%= @praca.area %></dd>
  <dt><strong><%= model_class.human_attribute_name(:perimetro) %>:</strong></dt>
  <dd><%= @praca.perimetro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @praca.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @praca.longitude %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              pracas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_praca_path(@praca), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              praca_path(@praca),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>


</section>